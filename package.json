{
  "name": "serverless-aws-node-sample",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "analyze": "npm run lint && npm run test",
    "clean": "rimraf .build .serverless .nyc_output coverage",
    "prebuild": "npm run clean && npm run analyze",
    "build": "serverless package",
    "predeploy": "npm run clean && npm run analyze",
    "deploy": "serverless deploy",
    "deploy:init": "serverless create_domain",
    "start": "serverless offline start",
    "pretest": "npm run clean",
    "test": "jest --colors --coverage --no-cache --runInBand",
    "test:watch": "jest --watchAll --colors --coverage --no-cache --runInBand",
    "test:ci": "jest --ci --colors --coverage --no-cache --runInBand --passWithNoTests --coverageReporters=text-lcov | coveralls",
    "lint": "tslint --project tsconfig.json **/*.ts"
  },
  "keywords": [
    "aws",
    "lambda",
    "node",
    "serverless",
    "typescript"
  ],
  "author": "Mads Engel Lundt <contact@elcc.dk>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/madslundt/serverless-aws-node-sample.git"
  },
  "sideEffects": false,
  "dependencies": {
    "joi": "^14.3.1",
    "request-promise-native": "^1.0.7"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.31",
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.16",
    "@types/joi": "^14.3.3",
    "@types/node": "^12.6.8",
    "@types/request": "^2.48.2",
    "@types/request-promise-native": "^1.0.16",
    "aws-lambda": "^0.1.2",
    "aws-sdk": "^2.503.0",
    "coveralls": "^3.0.5",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "faker": "^4.1.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "require-all": "^3.0.0",
    "rimraf": "^2.6.3",
    "serverless-aws-documentation": "^1.1.0",
    "serverless-domain-manager": "^3.2.6",
    "serverless-offline": "^5.8.0",
    "serverless-stack-output": "^0.2.3",
    "serverless-webpack": "^5.3.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tsconfig-paths-jest": "0.0.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.5.3",
    "webpack": "^4.38.0",
    "webpack-node-externals": "^1.7.2"
  }
}
